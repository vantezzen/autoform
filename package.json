{
  "name": "autoform",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev  --concurrency=12 --filter=!docs  --filter=!@autoform/joi --filter=!@autoform/core --filter=!@autoform/yup --filter=!@autoform/chakra --filter=!@autoform/mantine --filter=!@autoform/mui --filter=!@autoform/shadcn",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "cypress": "turbo run cypress --filter=web",
    "cypress:dev": "turbo run cypress --filter=web --parallel"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.1.3",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.2.4",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@changesets/cli": "^2.27.9"
  }
}
